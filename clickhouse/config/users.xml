<?xml version="1.0"?>
<yandex>
    <!-- 用户配置 -->
    <profiles>
        <!-- 默认配置 -->
        <default>
            <max_memory_usage>10000000000</max_memory_usage>
            <max_memory_usage_for_user>10000000000</max_memory_usage_for_user>
            <max_memory_usage_for_all_queries>20000000000</max_memory_usage_for_all_queries>
            <use_uncompressed_cache>0</use_uncompressed_cache>
            <load_balancing>random</load_balancing>
            <max_threads>8</max_threads>
            <max_insert_threads>0</max_insert_threads>
            <max_concurrent_queries_for_user>100</max_concurrent_queries_for_user>
            <max_execution_time>3600</max_execution_time>
            <max_ast_depth>1000</max_ast_depth>
            <max_ast_elements>10000</max_ast_elements>
            <max_expanded_ast_elements>100000</max_expanded_ast_elements>
            <max_bytes_before_external_group_by>20000000000</max_bytes_before_external_group_by>
            <max_bytes_before_external_sort>20000000000</max_bytes_before_external_sort>
        </default>

        <!-- 只读配置 -->
        <readonly>
            <readonly>1</readonly>
        </readonly>

        <!-- 写入优化配置 -->
        <write_optimized>
            <max_threads>1</max_threads>
            <max_insert_threads>1</max_insert_threads>
            <max_memory_usage>2000000000</max_memory_usage>
            <max_memory_usage_for_user>2000000000</max_memory_usage_for_user>
        </write_optimized>
    </profiles>

    <!-- 配额设置 -->
    <quotas>
        <!-- 默认配额 -->
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <result_rows>0</result_rows>
                <read_rows>0</read_rows>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>

    <!-- 用户列表 -->
    <users>
        <!-- 默认用户 -->
        <default>
            <password></password>
            <profile>default</profile>
            <quota>default</quota>
            <databases>
                <database>default</database>
            </databases>
        </default>

        <!-- 管理员用户 -->
        <admin>
            <password_sha256_hex>8c6976e5b5410415bde908bd4dee15dfb167a9c873fc4bb8a81f6f2ab448a918</password_sha256_hex>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
            <databases>
                <database>*</database>
            </databases>
            <networks incl="networks" replace="replace">
                <ip>::/0</ip>
            </networks>
        </admin>

        <!-- 只读用户 -->
        <readonly_user>
            <password>readonly_pass</password>
            <profile>readonly</profile>
            <quota>default</quota>
            <databases>
                <database>*</database>
            </databases>
            <networks incl="networks" replace="replace">
                <ip>::/0</ip>
            </networks>
        </readonly_user>

        <!-- 数据写入用户 -->
        <write_user>
            <password>write_pass</password>
            <profile>write_optimized</profile>
            <quota>default</quota>
            <databases>
                <database>*</database>
            </databases>
            <networks incl="networks" replace="replace">
                <ip>::/0</ip>
            </networks>
        </write_user>
    </users>

    <!-- 网络设置 -->
    <networks>
        <ip>::/0</ip>
    </networks>
</yandex>